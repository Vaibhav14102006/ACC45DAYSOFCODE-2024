QUESTION 2
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t, a, tatal=0;
	cin>>t;
	while(t-->0){
	    cin>>a;
	    tatal=10-a;
	    if(tatal>=3)
	    {
	        cout<<"YES"<<endl;
	    }else
	    {
	        cout<<"NO"<<endl;
	    }
	}
	return 0;

}
QUESTION 1
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t, a, b;
    cin >> t;
    while (t-- > 0) {
        cin >> a >> b;
        if (a % 6 == 0) {
            cout << (a / 6) * b << endl;
        } else {
            cout << ((a / 6) + 1) * b << endl;
        }
    }
    return 0;
}
